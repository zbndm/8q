{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "no-only-tests"],
  "ignorePatterns": ["node_modules", "dist", "scripts", "examples"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-only-tests/no-only-tests": "warn"
  }
}
